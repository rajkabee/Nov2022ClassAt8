package com.swingGui;

import java.awt.Toolkit;

import javax.swing.JFrame;

public class Frame {
	public Frame() {
		
		setTitle("Apple");
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setBounds(200, 100, 800, 500);
		setIconImage(Toolkit.getDefaultToolkit().getImage("logo.jpg"));
		
		
		
	}
}
